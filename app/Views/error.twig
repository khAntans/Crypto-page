{% extends "partials/base.html.twig" %}
{% block Title %}{{ errorCode }}{% endblock %}
{% block body %}
    <div class="text-center container-sm">
        <h1>{{ errorCode }}</h1>
        <h5>{{ errorMessage }}</h5>
        {% if errorCode == 404 %}
            <p class="text-muted">Ohh nothing was found :(
                <br>{{ possibleIssue }}</p>
        {% endif %}
    </div>
{% endblock %}